  <SQLQuery>
    <![CDATA[SELECT *
FROM ( SELECT POC_ID,
FOB_ID,
SYSTEM_ID,
BUYER_ID,
SEASON,
STYLE,
WASH_NAME,
COLOR,
BPO_ID,
BPO,
SUM (QTY) QTY,
FOB,
TO_CHAR (SHIPDATE, 'MM/DD/YYYY') SHIPDATE,
CLASSIFICATION,
PO_TYPE,
PARENT,
BPO_STATUS,
INCOTERMS,
TO_CHAR (ENDDATE, 'MM/DD/YYYY') ENDDATE,
SHIPMODE,
COUNTRY,
1 SL_NO
FROM XX_ORDER_INFO_V v
WHERE BPO_STATUS = 1 --AND SHIPMODE IS NOT NULL
GROUP BY POC_ID,
FOB_ID,
SYSTEM_ID,
BUYER_ID,
SEASON,
STYLE,
WASH_NAME,
COLOR,
BPO_ID,
BPO,
FOB,
SHIPDATE,
CLASSIFICATION,
PO_TYPE,
PARENT,
BPO_STATUS,
INCOTERMS,
ENDDATE,
SHIPMODE,
COUNTRY
UNION ALL
SELECT POC_ID,
FOB_ID,
SYSTEM_ID,
BUYER_ID,
SEASON,
STYLE,
WASH_NAME,
COLOR,
0 BPO_ID,
'' BPO,
SUM (QTY) QTY,
AVG(FOB),
'' SHIPDATE,
'' CLASSIFICATION,
'' PO_TYPE,
SUM (QTY)*AVG(FOB) PARENT,
0 BPO_STATUS,
'' INCOTERMS,
'' ENDDATE,
'' SHIPMODE,
'' COUNTRY,
2 SL_NO
FROM XX_ORDER_INFO_V v
WHERE BPO_STATUS = 1 --AND SHIPMODE IS NOT NULL
GROUP BY POC_ID,
FOB_ID,
SYSTEM_ID,
BUYER_ID,
SEASON,
STYLE,
WASH_NAME,
COLOR)
WHERE POC_ID = :1
ORDER BY POC_ID,
FOB_ID,
BUYER_ID,
SEASON,
STYLE,
WASH_NAME,
COLOR,
SL_NO ASC,
PARENT DESC]]>
  </SQLQuery>
